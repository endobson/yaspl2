(module hello-world
  (import
    (prim
      #:types (Array Bytes InputPort Int OutputPort)
      #:values (= write-bytes)
      #:patterns ()))
  (export)
  (types)

  (define (main [args : (Array Bytes)] [stdin : InputPort] [stdout : OutputPort] [stderr : OutputPort]) : Int
    (if (= (write-bytes #"hello world\n" stdout 0 12) 12)
        0
        1)))
