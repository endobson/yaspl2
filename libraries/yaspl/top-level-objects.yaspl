#:module (yaspl top-level-objects)
#:import {
  (list)
  (module-signature)
  (prim)
  (register-language) {
    #:types
      [FunctionDefinition r:FunctionDefinition]
  }
  (top-level-name)
}
(export
  #:types (TopLevelObject CompiledModule)
  #:values (register-function-tlo bytes-tlo trivial-variant-tlo trivial-closure-tlo
            compiled-module)
  #:patterns (register-function-tlo bytes-tlo trivial-variant-tlo trivial-closure-tlo
              compiled-module))
(types
  (define-type CompiledModule
    (compiled-module
      [code : (List TopLevelObject)]
      [sig : ModuleSignature]))
  (define-type TopLevelObject
    (register-function-tlo [v : r:FunctionDefinition])
    (bytes-tlo [name : Bytes] [value : Bytes])
    (trivial-variant-tlo [name : Bytes] [variant-tag : Int])
    ;; TODO remove the top level name from here once it is not needed for known funcs
    (trivial-closure-tlo [name : Bytes] [fun-name : Bytes] [top : TopLevelName])))
