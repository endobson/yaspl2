#:module (yaspl top-level-objects)
#:import {
  (prim)
  (stack-machine)
}
(export
  #:types (TopLevelObject)
  #:values (function-tlo trivial-closure-tlo bytes-tlo)
  #:patterns (stack-function function-tlo bytes-tlo trivial-closure-tlo))
(types
