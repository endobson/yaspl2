}
(export)
(types)

(define (main) : Int
  ((hide helper1) 100 #t 200))

(define (helper1 [x : Int] [y : Boolean] [z : Int]) : Int
  (if y ((hide helper2) 300 x) ((hide helper2) z 400)))

(define (helper2 [x : Int] [y : Int]) : Int
  y)

