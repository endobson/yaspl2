(module prim
  (define (fill-stack [x : int64]) : int64
    (def array1 (allocate-array undetermined-pointer 1))
    (def array2 (allocate-array undetermined-pointer 1))
    (def ptr1 (array-ref undetermined-pointer array1 0))
    (def ptr2 (cast (unrefine-pointer undetermined-pointer)
                    (array-ref undetermined-pointer array2 0)))
    (ptr-set! ptr1 ptr2)
    (return 0)))
