#:module (formats pe)
#:import {
  (either)
  (io)
  (prim)
}
(export
  (#:types PeFile)
  (#:values read-pe)
  (#:patterns))
(types
  (define-type PeFile
    (pe-file
      [section-headers : CoffHeader]
      [section-header-names-index : Int]
      [raw-contents : Bytes]))

  (define-type CoffHeader))


(define (read-pe [input : InputPort]) : (Either Bytes PeFile)
  (match-define raw-contents (read-all-bytes input))
  (left #"Not Yet Implemented"))
