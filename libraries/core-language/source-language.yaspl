#:module (core-language source-language)
#:import {
  (list)
  (prim)
}
(export
  (#:types Module Definition)
  (#:values module function-definition)
  (#:patterns))
(types
  (define-type Module
    (module [name : Bytes]
            [definitions : (List Definition)]))

  (define-type Definition
    (function-definition [name : Bytes])))
