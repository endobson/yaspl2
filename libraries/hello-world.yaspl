(module hello-world
  (import
    (prim
      #:types (Bytes InputPort OutputPort S64 Array)
      #:values (write-bytes =)
      #:patterns ()))
  (export)
  (types)

  (define (main [args : (Array Bytes)] [stdin : InputPort] [stdout : OutputPort] [stderr : OutputPort]): S64
    (if (= (write-bytes #"hello world\n" stdout 0 12) 12)
        0
        1)))
