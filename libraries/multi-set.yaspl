(module multi-set
  (import
    (prim
      #:types (Byte Boolean)
      #:values (+)
      #:patterns ())
    (list
      #:types (List)
      #:values ()
      #:patterns ())
    (dict
      #:types (Dict)
      #:values (dict-set make-dict dict-update dict-keys)
      #:patterns ()))
  (export make-multi-set multi-set-add multi-set-keys)
  (types
    (define-type (MultiSet a)
      (multi-set [counts (Dict a Byte)])))

  (define (a) (make-multi-set [eq : (a a -> Boolean)]) : (MultiSet a)
    (multi-set (make-dict eq)))

  (define (a) (multi-set-add [ms : (MultiSet a)] [e : a]) : (MultiSet a)
    (case ms
      [(multi-set counts)
       (multi-set (dict-update counts e (lambda ([x : Byte]) (+ x 1)) 0))]))

  (define (a) (multi-set-keys [ms : (MultiSet a)]) : (List a) 
    (dict-keys (multi-set-counts ms))))
