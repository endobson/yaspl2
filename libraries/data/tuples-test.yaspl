#:module (tuples-test)
#:import {
  (bytes)
  (list)
  (maybe)
  (prim)
  (tuples)
  (yunit)
}
(export
  (#:values main))
(types)

(define (main [args : Bytes] [stdin : InputPort] [stdout : OutputPort] [stderr : OutputPort]) : Int
  (yunit/main stderr
    (varargs list
      (new-test-case #"tuple2 equal (#\"1\" #\"a\") (#\"1\" #\"a\")"
        (lambda ()
          (if ((tuple2=? bytes=? bytes=?) (tuple2 #"1" #"a") (tuple2 #"1" #"a"))
              (nothing)
              (just (failure #"Tuples are unequal")))))
      (new-test-case #"tuple2 not equal (#\"1\" #\"a\") (#\"1\" #\"b\")"
        (lambda ()
          (if ((tuple2=? bytes=? bytes=?) (tuple2 #"1" #"a") (tuple2 #"1" #"b"))
              (just (failure #"Tuples are equal"))
              (nothing))))
      (new-test-case #"tuple2 not equal (#\"1\" #\"a\") (#\"2\" #\"a\")"
        (lambda ()
          (if ((tuple2=? bytes=? bytes=?) (tuple2 #"1" #"a") (tuple2 #"2" #"a"))
              (just (failure #"Tuples are equal"))
              (nothing)))))))
