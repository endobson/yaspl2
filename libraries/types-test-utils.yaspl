#:module (types-test-utils)
#:import {
  (list)
  (module-name)
  (types)
}
(export
  (#:types)
  (#:values bytes-type int-type list-type)
  (#:patterns))
(types)

(define (bytes-type) : Type
  (abstract-type (mod-name (varargs list #"prim")) #"Bytes" (empty) (star-kind)))

(define (int-type) : Type
  (abstract-type (mod-name (varargs list #"prim")) #"Int" (empty) (star-kind)))

(define (list-type [t : Type]) : Type
  (abstract-type (mod-name (varargs list #"list")) #"List" (varargs list t) (star-kind)))
