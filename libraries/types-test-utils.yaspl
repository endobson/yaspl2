#:module (types-test-utils)
#:import {
  {(list)
    {#:values
      empty
      list
    }
  }
  {(module-name)
    {#:values
      mod-name
    }
  }
  {(types)
    {#:types
      Type
    }
    {#:values
      abstract-type
    }
  }
}
(export
  (#:types)
  (#:values bytes-type int-type list-type)
  (#:patterns))
(types)

(define (bytes-type) : Type
  (abstract-type (mod-name (varargs list #"prim")) #"Bytes" (empty)))

(define (int-type) : Type
  (abstract-type (mod-name (varargs list #"prim")) #"Int" (empty)))

(define (list-type [t : Type]) : Type
  (abstract-type (mod-name (varargs list #"list")) #"List" (varargs list t)))
