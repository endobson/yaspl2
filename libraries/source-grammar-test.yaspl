#:module (source-grammar-test)
#:import {
  (list)
  (maybe)
  (prim)
  (source-grammar)
  (yunit)
}
(export)
(types)

(define (main [args : (Array Bytes)] [stdin : InputPort] [stdout : OutputPort] [stderr : OutputPort]) : Int
  (yunit/main stderr
    (varargs list
      (new-test-case #"Readable Grammar"
        (lambda () : (Maybe FailedAssertion)
          (if (= 0 (bytes-length (source-grammar)))
              (just (failure #"Empty grammar"))
              (nothing)))))))
