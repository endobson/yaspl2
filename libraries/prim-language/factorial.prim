(module factorial
  (define (f x)
    (return (* (+ x x) x)))

  (define (g x y)
    (def a x)
    (if (= y x)
        (then
          (set! a (+ y y)))
        (else))
    (def z (+ y x))
    (return (* (+ z x) x)))

  (define (h x)
    (def a x)
    (set! a (+ x x))
    (if (= a x)
        (then)
        (else))
    (return a))
  )
