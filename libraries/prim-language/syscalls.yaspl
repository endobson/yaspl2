#:module syscalls
#:import {
  prim {
    #:types
      Bytes
      Int
  }
}
(export
  #:types (Syscall)
  #:values (exit-syscall syscall-number syscall-name)
  #:patterns ())
(types
  (define-type Syscall
    (exit-syscall)))

(define (syscall-number [s : Syscall]) : Int
  (case s
    [(exit-syscall) #x2000001]))


(define (syscall-name [s : Syscall]) : Bytes
  (case s
    [(exit-syscall) #"exit"]))
