#:module (system-abi)
#:import {
  (prim)
}
(export
  (#:types SystemAbi)
  (#:values osx-abi linux-abi yasplos-abi current-system-abi)
  (#:patterns osx-abi linux-abi yasplos-abi))
(types
  (define-type SystemAbi
    (osx-abi)
    (linux-abi)
    (yasplos-abi)))

(define (current-system-abi) : SystemAbi
  (case (current-system-abi-int)
    [1 (osx-abi)]
    [2 (linux-abi)]
    [_ (panic #"Unknown abi")]))
